@import "basic"

#bg
  position fixed
  width 100%
  height 100%
  overflow hidden
  z-index 5

#bgFloor
  position absolute
  top: 50%
  opacity .2

.bgBlock
  position absolute
  width 20px
  height 20px

.bgBlock1
  @extend .bgBlock
  background colorGray1

.bgBlock2
  @extend .bgBlock
  background colorGray2